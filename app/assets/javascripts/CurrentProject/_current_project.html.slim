link href="/assets/ng-tags-input/ng-tags-input.min.css" rel="stylesheet" type="text/css"

tags-input class="bootstrap" ng-model="requirements" display_property="name"
    auto-complete source="loadTags()"
span E.g. 'CSC148 Grade', 'Volunteer', 'Resume', 'Unofficial transcript', etc.

div class="row"
    div class="col-xs-4"


        div class="text-center"
            h3 Current Projects

            /.btn-group dropdown="" is-open="status.isopen"
            /    button#single-button.btn.btn-primary dropdown-toggle="" ng-disabled="disabled" type="button"
            /        | {{ filter }}
            /        span.caret
            /    ul.dropdown-menu aria-labelledby="single-button" role="menu"
            /        li role="menuitem"
            /            a ng-click="pagenumber.inProgress = 0;filter = 'In progress';flipInProgress(10)" In progress
            /        li role="menuitem"
            /            a ng-click="pagenumber.completed = 0;filter = 'Completed';flipCompleted(10)" Completed



        br

        div class="text-center"
            div class="btn-group"
                button class="btn btn-default" ng-click="flip(-10)" Prev
                button class="btn btn-default disabled"
                    | {{ pagenumber.current }}
                button class="btn btn-default" ng-click="flip(10)" Next
            br
            br
        div class="list-group"
            div ng-repeat="project in projects"
                ul class="nav nav-pills nav-stacked"
                    li
                        a ui-sref="current_project.selected_project({id: project.id})"
                            | {{ project.title }}

    div class="row" style="height:5px"

    div class="col-xs-6"
        div class="panel-content"
            ui-view

    div class="col-xs-2"
        div class="row" style="height:15px"

        div class="btn-group" role="group" aria-label="Default button group"
            /Button 1: Information
            button ng-click="modalService.open('CurrentProject/CurrentProjectSettings/current_project_info.html', 'CurrentProjectSettingsCtrl', items)" type="button" class="btn btn-default" data-toggle="tooltip" data-placement="bottom" title="Information"
                span class="glyphicon glyphicon-info-sign" aria-hidden="true"

            /Button 2: Account Settings
            button ng-click="modalService.open('CurrentProject/CurrentProjectSettings/current_project_settings.html', 'CurrentProjectSettingsCtrl', items)"type="button" class="btn btn-default" data-toggle="tooltip" data-placement="bottom" title="Settings"
                span class="glyphicon glyphicon-tasks" aria-hidden="true"
