link href="/assets/ng-tags-input/ng-tags-input.min.css" rel="stylesheet" type="text/css"

div class="row"
    div class="col-xs-4"


        div class="text-center"
            h3 Current Projects
            span Refine your search:
            tags-input class="bootstrap" ng-model="requirements" display_property="name"
                auto-complete source="loadTags($query)" on-tag-added="filterProjects(true, 0)"
            /button class="btn btn-default" ng-click="filterProjects()" Search


        br

        div class="text-center"
            div class="btn-group"
                button class="btn btn-default" ng-click="filterProjects(false, -10)" Prev
                button class="btn btn-default disabled"
                    | {{ pagenumber.current }}
                button class="btn btn-default" ng-click="filterProjects(false, 10)" Next
            br
            br
        div class="list-group"
            div ng-repeat="project in projects"
                ul class="nav nav-pills nav-stacked"
                    li
                        a ui-sref="current_project.project_info({id: project.id})"
                            | {{ project.title }}

    div class="row" style="height:5px"

    div class="col-xs-6"
        div class="panel-content"
            ui-view

    div class="col-xs-2"
        div class="row" style="height:15px"

        div class="btn-group" role="group" aria-label="Default button group"
            /Button 1: Information
            button ng-click="modalService.open('InfoPopup/info.html', 'InfoCtrl')" type="button" class="btn btn-default" data-toggle="tooltip" data-placement="bottom" title="Information"
                span class="glyphicon glyphicon-info-sign" aria-hidden="true"

